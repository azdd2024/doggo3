{
  "name": "doggo-cms",
  "version": "1.0.0",
  "description": "Doggo Platform CMS Backend",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "payload": "tsx src/payload.config.ts"
  },
  "dependencies": {
    "@doggo/database": "*",
    "@doggo/types": "*",
    "@doggo/utils": "*",
    "payload": "^2.8.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "socket.io": "^4.7.4",
    "ioredis": "^5.3.2",
    "bull": "^4.12.0",
    "nodemailer": "^6.9.7",
    "@sendgrid/mail": "^7.7.0",
    "twilio": "^4.19.0",
    "stripe": "^14.9.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "@aws-sdk/client-s3": "^3.454.0",
    "@aws-sdk/s3-request-presigner": "^3.454.0",
    "sharp": "^0.33.0",
    "qrcode": "^1.5.3",
    "pdf-lib": "^1.17.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "@sentry/node": "^7.85.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/multer": "^1.4.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/qrcode": "^1.5.5",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.6.0",
    "copyfiles": "^2.4.1",
    "typescript": "^5.3.0"
  }
}